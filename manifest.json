{
	"name": "Switcheroo-nscape",
	"version": "1.0",
	"description": "Redirects old RuneScape wiki pages to new RuneScape wiki pages",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "Switcheroo-nscape"
	},
	"permissions": [
		"storage"
		// "webRequest",
		// "*://*.google.com/"
	],
	"content_scripts": [
		{
			"matches": [
				"*://runescape.fandom.com/wiki/*"
			],
			"js": ["content.js"]
		}
		
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
